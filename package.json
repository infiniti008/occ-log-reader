  "version": "1.0.0",
  "description": "Reading logs for SSE from OCCS environment.",
  "main": "index.js",
  "scripts": {},
  "author": "Vyacheslav Nikitenok",
  "license": "ISC",
  "dependencies": {
    "request": "^2.88.0"
  },
  "homepage": "https://github.com/infiniti008/occ-log-reader",
