{
	"name": "occ-logs",
	"displayName": "occ-logs",
	"description": "Read SSE logs from occ srver",
	"version": "0.8.0",
	"publisher": "infiniti008",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.readLogs",
		"onCommand:extension.configureLogs",
		"onCommand:extension.cloneConfig",
		"onCommand:extension.readWithConfig"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.readLogs",
				"title": "occ-logs | Reading logs"
			},
			{
				"command": "extension.configureLogs",
				"title": "occ-logs | Configure logs"
			},
			{
				"command": "extension.cloneConfig",
				"title": "occ-logs | Clone config"
			},
			{
				"command": "extension.readWithConfig",
				"title": "occ-logs | Read with configuring"
			}
		]
	},
	"scripts": {
		"build": "vsce package"
	},
	"dependencies": {
		"occ-log-reader-package": "git+https://github.com/infiniti008/occ-log-reader.git#semver:^v1.0.0"
	},
	"devDependencies": {
		"infiniti008-eslint-config": "git+https://github.com/infiniti008/eslint-config.git",
		"@types/glob": "^7.1.1",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"typescript": "^3.3.1"
	},
	"eslintConfig": {
		"extends": [
			"./node_modules/infiniti008-eslint-config",
			"./node_modules/infiniti008-eslint-config/ext/node"
		]
	},
	"repository": {
		"type" : "git",
		"url" : "https://github.com/infiniti008/occ-log-reader.git#vscode-pkg"
	  }
}
